{
    "name": "Codeanywhere PHP Template",
    "build": {
      "dockerfile": "Dockerfile"
    },
    "features": {
      "ghcr.io/devcontainers/features/common-utils:1": {
        "installZsh": "true",
        "username": "codeany",
        "uid": "1000",
        "gid": "1000",
        "upgradePackages": "false"
      },
      "ghcr.io/devcontainers/features/docker-in-docker:1.0.8": {
        "version": "latest",
        "moby": true
      },
      "ghcr.io/devcontainers/features/node:1.1.1": {
        "version": "18.12.1"
      },
      "ghcr.io/devcontainers/features/php:1.0.9": {}
    },
    "remoteUser": "codeany"
  }
  